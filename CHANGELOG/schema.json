{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "message": {
      "type": "string",
      "description": "",
      "minLength": 0,
      "maxLength": 1024
    },
    "type": {
      "type": "string",
      "description": "Type of change",
      "enum": [
        "feat",
        "fixed",
        "chore"
      ]
    },
    "pr": {
      "type": "integer"
    }
  },
  "required": [
    "message",
    "type",
    "pr"
  ]
}
